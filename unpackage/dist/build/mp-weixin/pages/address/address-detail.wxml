<view><map class="map"></map><form data-event-opts="{{[['submit',[['handleSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="cu-form-group margin-top"><view class="title"><text class="cuIcon-friendfill"></text></view><input class="text-right" name="username" value="{{address.username}}" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['address']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title"><text class="cuIcon-mobilefill"></text></view><input class="text-right" name="phone" value="{{address.phone}}" placeholder="请输入电话" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['address']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title"><text class="cuIcon-locationfill"></text></view><picker range="{{regionArr}}" data-event-opts="{{[['change',[['handleRegion',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(regionIdx==-1?'请选择区域':regionArr[regionIdx])+''}}</view></picker></view><view class="cu-form-group"><view class="title"><text class="cuIcon-locationfill"></text></view><input class="text-right" name="detail" value="{{address.detail}}" placeholder="请输入详细地址" data-event-opts="{{[['input',[['__set_model',['$0','detail','$event',[]],['address']]]]]}}" bindinput="__e"/></view><view class="padding"><button class="cu-btn bg-brown block" form-type="submit">确定</button></view></form></view>